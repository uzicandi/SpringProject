<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>
		<typeAlias alias="Board" 		type="oracle.java.bmw.model.Board"/>
		<typeAlias alias="Cart" 		type="oracle.java.bmw.model.Cart"/>
		<typeAlias alias="Category" 	type="oracle.java.bmw.model.Category"/>
		<typeAlias alias="Clip" 		type="oracle.java.bmw.model.Clip"/>
		<typeAlias alias="Comments" 	type="oracle.java.bmw.model.Comments"/>
