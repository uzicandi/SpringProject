		<mapper resource="mybatis/Clip.xml" />
		<mapper resource="mybatis/Comments.xml" />
		<mapper resource="mybatis/Ingredient.xml" />
		<mapper resource="mybatis/IngtList.xml" />
		<mapper resource="mybatis/ItemInfo.xml" />
		<mapper resource="mybatis/Member.xml"/>
		<mapper resource="mybatis/Point.xml"/>
		<mapper resource="mybatis/Ranking.xml" />		
		<mapper resource="mybatis/SaveFiles.xml"/>	
		<mapper resource="mybatis/Utility.xml"/>
		<mapper resource="mybatis/Shopping.xml"/>
	</mappers>
</configuration>

